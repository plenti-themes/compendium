{
  "build": "deploy/public",
  "baseurl": "/compendium/",
  "local": {
    "port": 3000
  },
  "routes": {
    "404": ":filename",
    "index": ":paginate(totalPages)",
    "pages": "pages/:filename",
    "posts": "posts/:filename",
    "projs": ":filename/:paginate(totalProjPages)",
    "tags": ":filename/:paginate(totalTagsPages)",
    "catgs": ":filename/:paginate(totalCatgsPages)"
  }
}
